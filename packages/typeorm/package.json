{
  "name": "@next-auth/typeorm-adapter",
  "repository": "https://github.com/nextauthjs/adapters",
  "version": "0.0.0",
  "description": "TypeORM adapter for next-auth.",
  "author": "Iain Collins",
  "license": "ISC",
  "keywords": [
    "next-auth",
    "next.js",
    "oauth",
    "typeorm"
  ],
  "homepage": "https://next-auth.js.org",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "main": "dist/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test:app:start": "docker-compose -f tests/docker/app.yml up -d",
    "test:app:rebuild": "docker-compose -f tests/docker/app.yml up -d --build",
    "test:app:stop": "docker-compose -f tests/docker/app.yml down",
    "test": "yarn run test:app:rebuild && yarn run test:app:stop",
    "test:db": "yarn run test:db:mysql && yarn run test:db:postgres && yarn run test:db:mongodb && yarn run test:db:mssql",
    "test:db:mysql": "node test/mysql.js",
    "test:db:postgres": "node test/postgres.js",
    "test:db:mongodb": "node test/mongodb.js",
    "test:db:mssql": "node test/mssql.js",
    "db:start": "docker-compose -f test/docker/databases.yml up -d",
    "db:stop": "docker-compose -f test/docker/databases.yml down",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "dependencies": {
    "crypto-js": "^4.0.0",
    "require_optional": "^1.0.1",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lru-cache": "^5.1.0",
    "@types/next-auth": "^3.1.23",
    "assert": "^2.0.0",
    "jest": "^26.6.3",
    "next-auth": "^3.1.0",
    "standard": "^16.0.3"
  },
  "peerOptionalDependencies": {
    "next-auth": "^3.1.0",
    "mongodb": "^3.5.9",
    "mysql": "^2.18.1",
    "mssql": "^6.2.1",
    "pg": "^8.2.1"
  },
  "standard": {
    "ignore": [
      "test/",
      "pages/",
      "components/"
    ]
  }
}
